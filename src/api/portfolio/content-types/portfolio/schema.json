{
  "kind": "collectionType",
  "collectionName": "portfolios",
  "info": {
    "singularName": "portfolio",
    "pluralName": "portfolios",
    "displayName": "Portfolio"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "nama": {
      "type": "string",
      "required": true
    },
    "nama_highlighted": {
      "type": "string"
    },
    "thumbnail": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "jenis": {
      "type": "enumeration",
      "enum": [
        "online",
        "design only"
      ]
    },
    "url": {
      "type": "string"
    },
    "penanda": {
      "type": "string"
    },
    "is_active": {
      "type": "boolean",
      "required": true,
      "default": false
    },
    "is_highlighted": {
      "type": "boolean",
      "default": false
    },
    "urutan": {
      "type": "integer",
      "unique": true,
      "min": 1
    }
  }
}
